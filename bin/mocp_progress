#!/usr/bin/env python

from subprocess import Popen, PIPE

cmd = 'mocp -i'.split(' ')
p = Popen(cmd, stdout=PIPE)
info = str(p.stdout.read(), 'utf-8').split('\n')
d = dict([tuple(t.strip().split(':', 1)) for t in info[:-1]])

total = d['TotalTime'].split(':')
total = int(total[0])*60 + int(total[1])
cur = d['CurrentTime'].split(':')
cur = int(cur[0])*60 + int(cur[1])

print(cur/total*100)
