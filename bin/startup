#!/usr/bin/env zsh



if [[ "$(uname)" == "FreeBSD" ]]; then
else
    /usr/local/bin/mozc start
    nm-applet
    ~/bin/beepoff
    xset -b

    sleep 2
    conky -q --config=~/.conkyrc
    octopi-notifier

    ibus-daemon
    volumeicon
    anamnesis --start
    unclutter
fi

feh --bg-scale ~/Pictures/background.png
~/bin/dvorak
~/bin/sands
~/bin/reset_yeah
sxhkd
sleep 10s && redshift -c ~/.config/redshift.conf
dunst -config $HOME/.config/dunst/dunstrc
