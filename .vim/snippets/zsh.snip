snippet     if
abbr        [[...]]
options     head
    if [[ ${1} ]]; then
        ${2}
    fi
    ${0}

snippet     range
abbr        {i..n}
    {${1}..${2}}

snippet     math
abbr        math
options     head
    zmodload zsh/mathfunc


snippet     argc
abbr        $ARGC
options     head
    $#

